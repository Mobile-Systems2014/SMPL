<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smpl"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.smpl.objectListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.smpl.objectDetailActivity"
            android:label="@string/title_object_detail"
            android:parentActivityName="com.example.smpl.objectListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.smpl.objectListActivity" />
        </activity>
        <activity
            android:name="com.application.smpl.qr_reader.DecoderActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.smpl.DatabaseDebug"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.example.smpl.StoreMap"
            android:label="Map"
            android:screenOrientation="landscape">
        </activity>
    </application>

</manifest>